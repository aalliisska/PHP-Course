{% extends "base.html.twig" %}

{% block title %}All registred users{% endblock %}
{% block head %}
    {{ parent() }}
{% endblock %}

{% block view_users %}
    <h1>Registered users data</h1>
    <div>
        <ul>
            {% for user in users %}
                <li>
                    {{ path("show_user", {user_id: user.userId}) }}">{{ user.firstName }} {{ user.lastName }}
                </li>
        </ul>

    </div>

{% endblock %}

